pipeline {
    agent { label "linux" }
    tools {
        maven "MAVEN"
        jdk "JDK"
    }

    stages {
        stage('Check-In') {
            steps {
                sh 'git 'https://github.com/daticahealth/java-war-deploy-example.git''
            }
        }
        stage('Initialize') {
            steps {
                echo "PATH = ${M2_HOME}/bin:${PATH}"
                echo "M2_HOME = /opt/maven"
            }
        }
    }
}
